{
    "name": "obada-protocol/php-sdk",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/obada-protocol/php-client-library.git"
        }
    ],
    "require": {
        "php": ">=7.4",
        "tuupola/base58": "^2.1",
        "obada-protocol/php-client-library": "*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0"
    },
    "autoload": {
        "psr-4": { "Obada\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Obada\\Test\\" : "tests/" }
    }
}
